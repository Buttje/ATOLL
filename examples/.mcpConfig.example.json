{
  "servers": {
    "example-stdio": {
      "transport": "stdio",
      "command": "python",
      "args": ["path/to/your/mcp_server.py"],
      "timeoutSeconds": 30,
      "env": {
        "ENV_VAR": "value"
      }
    },
    "example-http": {
      "transport": "streamable_http",
      "url": "http://localhost:8080",
      "timeoutSeconds": 30
    }
  }
}
