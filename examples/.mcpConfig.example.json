{
  "servers": {
    "example-stdio": {
      "type": "stdio",
      "command": "python",
      "args": ["path/to/your/mcp_server.py"],
      "cwd": "${workspaceFolder}",
      "env": {
        "ENV_VAR": "value"
      }
    },
    "example-http": {
      "type": "http",
      "url": "http://localhost:8080/mcp",
      "headers": {
        "Authorization": "Bearer YOUR_TOKEN_HERE"
      }
    },
    "example-sse": {
      "type": "sse",
      "url": "http://localhost:8080/events"
    }
  }
}
